<?xml version="1.0" encoding="UTF-8"?> 

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml
		  /ns/javaee/web-app_2_5.xsd"
		     version="2.5">
			 
<servlet>
	<servlet-name>Status Home</servlet-name>
	<servlet-class>Welcome</servlet-class>
		<init-param>
			<param-name>statusIndex</param-name>
			<param-value>web/index.jsp</param-value>
		</init-param>
</servlet>

<servlet-mapping>
	<servlet-name>Status Home</servlet-name>
	<url-pattern>/</url-pattern>
</servlet-mapping>

</web-app>

